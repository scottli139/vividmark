{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        { "path": "$HOME/**" },
        { "path": "/Users/**" },
        { "path": "/Volumes/**" }
      ]
    }
  ]
}
